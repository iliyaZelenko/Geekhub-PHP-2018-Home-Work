{
  "defaultSeverity": "warning",
  "extends": ["tslint:latest"],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ],
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      "@/*": ["./src/*"]
    }
  },
  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": [false],
    "no-implicit-dependencies": [true, "dev"]
  }
}
